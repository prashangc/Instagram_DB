<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.db_successful">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.DB_successful">
        <activity android:name=".SignUp"></activity>
        <activity
            android:name=".SplashActivity">

        </activity>
        <activity
            android:exported="true"
            android:name=".Login">

            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name">

        </activity>

        <activity android:name=".activityProfile"
            android:label="Activity Profile"
            android:parentActivityName=".activityPost"
            />
        <activity android:name=".activityLike"></activity>
        <activity android:name=".activitySearch"></activity>
        <activity android:name=".activityPost"></activity>
        <activity android:name="com.example.db_successful.activityStory"></activity>


        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>